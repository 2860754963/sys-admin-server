{
  "name": "sys-admin-server",
  "version": "0.0.1",
  "description": "A simple management system ",
  "private": true,
  "bin": "./bin/www",
  "scripts": {
    "pinstall": "pnpm install",
    "dev": "dotenv -e .env.development supervisor ./bin/www",
    "prod": "dotenv -e .env.production supervisor ./bin/www",
    "pkg": "pkg . -t  node18-macos-arm64 --out-path dist/",
    "build:dev": "webpack --config webpack.config.js --mode=development",
    "build:prod": "webpack --config webpack.config.js --mode=production"
  },
  "pkg": {
    "assets": [
      "views/**/*",
      "public/**/*",
      ".env*",
      "config/**/*"
    ]
  },
  "dependencies": {
    "@alicloud/dysmsapi20170525": "4.1.1",
    "@babel/core": "7.21.5",
    "@babel/preset-env": "^7.28.0",
    "@xdooi/consoletable": "^1.0.3",
    "body-parser": "^2.2.0",
    "captchapng2": "^1.0.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "debug": "~2.2.0",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "express": "~5.1.0",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.14.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "~1.7.0",
    "multer": "^2.0.1",
    "mysql": "^2.18.1",
    "pkg": "^5.8.1",
    "pkg-fetch": "^3.5.2",
    "serve-favicon": "~2.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "babel-loader": "8.2.5",
    "dotenv-webpack": "^8.1.1",
    "express-session": "^1.14.2",
    "supervisor": "^0.12.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "engines": {}
}
